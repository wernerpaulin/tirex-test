    {
        "id": "0e58590a2a3ae7b8",
        "type": "function",
        "z": "9f842c3bab9bc650",
        "name": "convert into time series format",
        "func": "const context_csv = msg.payload\n\n// msg.payload is like: [{col1:112},{col1:118},...]\nconst context_data = context_csv.map((row) => Number(row.col1));\n\nmsg.payload = context_data\n\nreturn msg;",
