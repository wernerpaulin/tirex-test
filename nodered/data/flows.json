    {
        "id": "7e073c33149d997c",
        "type": "function",
        "z": "9f842c3bab9bc650",
        "name": "raw to UTF string",
        "func": "if (Buffer.isBuffer(msg.payload)) {\n  // single file as Buffer\n  msg.payload = msg.payload.toString('utf8');\n} else if (Array.isArray(msg.payload) && msg.payload.every(Buffer.isBuffer)) {\n  // multiple files -> join them (or handle separately)\n  msg.payload = msg.payload.map(b => b.toString('utf8')).join('\\n');\n} else if (msg.payload && msg.payload.buffer && Buffer.isBuffer(msg.payload.buffer)) {\n  // some nodes wrap it like { buffer, filename, mimetype }\n  msg.payload = msg.payload.buffer.toString('utf8');\n}\n// now it's a UTF-8 string\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
